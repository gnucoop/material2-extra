{
  "name": "material2-extra",
  "version": "0.0.1",
  "description": "Extra widget for Angular Material 2",
  "scripts": {
    "build": "node_modules/.bin/ng build",
    "demo-app": "node_modules/.bin/ng serve",
    "test": "node_modules/.bin/karma start test/karma.conf.js",
    "tslint": "node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "stylelint": "node_modules/.bin/stylelint 'src/**/*.scss' --config stylelint-config.json --syntax scss",
    "check-circular-deps": "node_modules/.bin/madge --circular ./dist",
    "typings": "node_modules/.bin/typings install --global",
    "postinstall": "npm run typings",
    "e2e": "node_modules/.bin/protractor",
    "inline-resources": "node ./scripts/release/inline-resources.js ./dist/components",
    "deploy": "node_modules/.bin/firebase deploy",
    "webdriver-manager": "node_modules/.bin/webdriver-manager"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.2",
    "@angular/compiler": "2.0.0-rc.2",
    "@angular/core": "2.0.0-rc.2",
    "@angular/platform-browser": "2.0.0-rc.2",
    "@angular/platform-browser-dynamic": "2.0.0-rc.2",
    "@angular2-material/button": "^2.0.0-alpha.5-2",
    "@angular2-material/core": "^2.0.0-alpha.5-2",
    "hammerjs": "^2.0.8",
    "moment": "^2.13.0",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.31",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "add-stream": "^1.0.0",
    "angular-cli": "^1.0.0-beta.8",
    "broccoli-autoprefixer": "^4.1.0",
    "broccoli-funnel": "^1.0.2",
    "broccoli-merge-trees": "^1.1.1",
    "browserstacktunnel-wrapper": "^1.4.2",
    "conventional-changelog": "^1.1.0",
    "ember-cli-inject-live-reload": "^1.4.0",
    "firebase-tools": "^3.0.3",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.5",
    "jasmine-core": "^2.4.1",
    "js-yaml": "^3.5.2",
    "karma": "^1.0.0",
    "karma-browserstack-launcher": "^1.0.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-sauce-launcher": "^1.0.0",
    "madge": "^0.5.3",
    "node-sass": "^3.4.2",
    "protractor": "^3.3.0",
    "protractor-accessibility-plugin": "0.1.1",
    "sass": "^0.5.0",
    "strip-ansi": "^3.0.0",
    "stylelint": "^6.7.1",
    "symlink-or-copy": "^1.0.1",
    "ts-node": "^0.9.3",
    "tslint": "^3.12.1",
    "typescript": "^1.9.0-dev",
    "typings": "^1.0.4",
    "which": "^1.2.4"
  }
}
